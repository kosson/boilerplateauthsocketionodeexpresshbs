<link rel="stylesheet" href="../../css/main.css" media="screen" title="no title" charset="utf-8">
<form class="" action="" onsubmit="return login()">
  <fieldset>
      <legend>Login</legend>
      <div>
          <label for="email">Email:</label>
          <input type="email"
                 id="email"
                 name="email"
                 value=""
                 aria-describedby="email-format"
                 required aria-required=”true”
                 data-validation-error-msg="Nu-i un mail valid">
          <span id="email-format" class="help">Introdu un email valid</span>
      </div>
      <div>
          <label for="password">Password:</label>
          <input type="password" id="password" name="password" value="">
      </div>
      <div class="submit">
          <input type="submit" name="loginFrm" value="Logare">
      </div>
      <a href="/api/signin">Fă cont nou</a>
  </fieldset>
</form>
<div id="mesaje"></div>
<script src="../../libs/jquery/dist/jquery.min.js" charset="utf-8"></script>
<script src="../../libs/jquery-form-validator/form-validator/jquery.form-validator.min.js" charset="utf-8"></script>
<script src="../js/com.js" charset="utf-8"></script>
<script src="../js/login.js" charset="utf-8"></script>
